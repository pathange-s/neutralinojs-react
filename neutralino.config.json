{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/myapp-react/build/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*"
  ],
  "modes": {
    "window": {
      "title": "myapp",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/myapp-react/public/logo192.png"
    }
  },
  "cli": {
    "binaryName": "myapp",
    "resourcesPath": "/www/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/www/neutralino.js",
    "binaryVersion": "4.4.0",
    "clientVersion": "3.3.0",
    "resourcesPath": "/myapp-react/build/",
    "clientLibrary": "/myapp-react/public/neutralino.js",
    "frontendLibrary": {
      "patchFile": "/myapp-react/public/index.html",
      "devUrl": "http://localhost:3000"
  }
  }
}